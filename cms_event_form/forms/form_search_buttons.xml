<odoo>


    <template id="ppz_form_buttons" name="CMS event form buttons" 
        inherit_id="cms_form.base_form">
        <xpath expr="//div[contains(@class, 'above-controls')]"
            position="before">
            <div id="controls_new_position">
            </div>
        </xpath>
        <xpath expr="//div[@class='form-group form-controls']"
            position="move" 
            target="//div[@id='controls_new_position']">
        </xpath>
    </template>


<!-- move the above-controls -->
</odoo>
